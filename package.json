{
  "name": "tictactoe",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe Game",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass scss/style.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass scss/style.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladalbescu/Tic-Tac-Toe.git"
  },
  "author": "Vlad Albescu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vladalbescu/Tic-Tac-Toe/issues"
  },
  "homepage": "https://github.com/vladalbescu/Tic-Tac-Toe#readme",
  "dependencies": {
    "node-sass": "^7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
